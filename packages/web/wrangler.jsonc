{
  "name": "web",
  "main": ".output/server/index.mjs",
  "compatibility_date": "2025-04-20",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": ".output/public",
    "binding": "ASSETS",
  },
  "services": [
    { "binding": "AUTH", "service": "auth-issuer" },
    { "binding": "API", "service": "api" },
  ],
}
