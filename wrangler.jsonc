{
  "name": "web",
  "main": "./packages/web/.output/server/index.mjs",
  "compatibility_date": "2025-04-20",
  "compatibility_flags": ["nodejs_compat"],
  "vars": { "VITE_API_URL": "https://api.johnroyal.workers.dev" },
  "assets": {
    "directory": "./packages/web/.output/public",
    "binding": "ASSETS"
  },
  "services": [
    { "binding": "AUTH", "service": "auth-issuer" },
    { "binding": "API", "service": "api" }
  ]
}
